{
  "questions": [
    { "question":  "ETL version: ",
      "env-var":  "ETL_IMAGE_TAG",
      "default-value": "v1.2.3"
    },
    { "question":  "Vocabulary schema: ",
      "env-var":  "SRC_VOCAB_DB_SCHEMA",
      "default-value": "omopcdm"
    },
    { "question":  "Current OMOP v5.3 source schema name: ",
      "env-var":  "SRC_DB_53_SCHEMA",
      "default-value": "omopcdm"
    },
    { "question":  "New OMOP v5.3 CDM source schema name: ",
      "env-var":  "RENAMED_SRC_DB_53_SCHEMA",
      "default-value": "omopcdm_53"
    },
    { "question":  "Target OMOP v5.4 CDM schema name: ",
      "env-var":  "TARGET_DB_54_SCHEMA",
      "default-value": "omopcdm"
    },
    { "question":  "Current OMOP v5.3 results schema name: ",
      "env-var":  "SRC_RESULTS_DB_53_SCHEMA",
      "default-value": "results"
    },
    { "question":  "New OMOP v5.3 results schema name: ",
      "env-var":  "RENAMED_SRC_RESULTS_DB_53_SCHEMA",
      "default-value": "results_53"
    },
    { "question":  "Target OMOP v5.4 results schema name: ",
      "env-var":  "TARGET_RESULTS_DB_54_SCHEMA",
      "default-value": "results"
    },
    { "question":  "Current OMOP v5.3 patient check schema name: ",
      "env-var":  "SRC_PATIENT_CHECK_DB_SCHEMA",
      "default-value": "results"
    },
    { "question":  "Run Data Quality Pipeline after conversion: ",
      "env-var":  "RUN_DQD",
      "allowed-values": "true,false",
      "default-value": "true"
    },
    { "question":  "Verbosity level of ETL logging: ",
      "env-var":  "VERBOSITY_LEVEL",
      "allowed-values": "INFO,DEBUG",
      "default-value": "INFO"
    }
    ]
}